original_string = "{[A].[A].[B]}*{[C].[C].[D]}*{[E].[E].[F]}"
parameter = "C"
replacement_list = ["{[G].[G].[H]}", "{[I].[I].[J]}"]

# Split the original string based on '*'
segments = original_string.split('*')

# Iterate over segments and replace the substring based on the parameter
updated_strings = []
for segment in segments:
    if parameter in segment:
        for replacement in replacement_list:
            updated_string = segment.replace(segment[segment.find('{'): segment.find('}') + 1], replacement)
            updated_strings.append(updated_string)
    else:
        updated_strings.append(segment)

print(updated_strings)
